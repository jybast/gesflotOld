<form method="post" action="{{ path('vehicule_delete', {'id': vehicule.id}) }}" onsubmit="return confirm({% trans %}'Are you sure you want to delete this item?'{% endtrans %});">
	<input type="hidden" name="_token" value="{{ csrf_token('delete' ~ vehicule.id) }}">
	<button class="btn">{% trans %}Delete{% endtrans %}</button>
</form>
